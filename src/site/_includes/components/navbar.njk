{%if settings.dgHomeLink === true%}
    <nav class="navbar">
        <div class="navbar-inner">
            <a href="/" class="site-brand" aria-label="Home">
                <img class="site-logo" src="/img/logo.svg" alt="" width="22" height="22" loading="eager" decoding="async" />
                <h1 class="site-title">{{meta.siteName}}</h1>
            </a>
        </div>
        {% if settings.dgEnableSearch === true%}
            {% include "components/searchButton.njk" %}
        {%endif%}
    </nav>
    {%else%}
    <div class="empty-navbar" >
        {% if settings.dgEnableSearch === true%}
            {% include "components/searchButton.njk" %}
        {%endif%}
    </div>
{%endif%}